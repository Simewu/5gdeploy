{
  "name": "@usnistgov/5gdeploy",
  "private": true,
  "type": "module",
  "scripts": {
    "clang-format": "git ls-files '*.cpp' | xargs clang-format-15 -i -style=file",
    "dockerfilelint": "git ls-files -- '*/Dockerfile' | xargs dockerfilelint",
    "lint": "xo-yoursunny --fix",
    "netdef-compose": "tsx ./netdef-compose/main.ts",
    "phoenix-compose": "tsx ./phoenix-compose/main.ts",
    "phoenix-rpc": "tsx ./phoenix-rpc/main.ts",
    "shfmt": "git ls-files -- '*.sh' | xargs docker run --rm -u $(id -u):$(id -g) -v $PWD:/mnt -w /mnt mvdan/shfmt:v3 -l -w -s -i=2 -ci",
    "typecheck": "tsc"
  },
  "packageManager": "pnpm@8.14.3",
  "dependencies": {
    "@nodelib/fs.walk": "^2.0.0",
    "@segment/ajv-human-errors": "^2.12.0",
    "@types/dockerode": "^3.3.23",
    "@types/js-yaml": "^4.0.9",
    "@types/minimalistic-assert": "^1.0.3",
    "@types/netmask": "^2.0.5",
    "@types/node": "^20.11.5",
    "@types/sql-tagged-template-literal": "^1.2.2",
    "@types/yargs": "^17.0.32",
    "ajv": "^8.12.0",
    "dockerfilelint": "^1.8.0",
    "dockerode": "^4.0.2",
    "envfile": "^7.1.0",
    "execa": "^8.0.1",
    "get-stdin": "^9.0.0",
    "jayson": "^4.1.0",
    "js-yaml": "^4.1.0",
    "json-stringify-deterministic": "^1.0.12",
    "minimalistic-assert": "^1.0.1",
    "minimatch": "^9.0.3",
    "mnemonist": "^0.39.7",
    "netmask": "^2.0.2",
    "p-event": "^6.0.0",
    "shlex": "^2.1.2",
    "sql-tagged-template-literal": "^1.2.0",
    "streaming-iterables": "^8.0.1",
    "strip-ansi": "^7.1.0",
    "ts-json-schema-generator": "^1.5.0",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "type-fest": "^4.10.0",
    "typescript": "~5.3.3",
    "uint8-util": "^2.2.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@yoursunny/xo-config": "0.56.2"
  },
  "engines": {
    "node": "^20.9.0"
  }
}
